<?xml version="1.0" encoding="UTF-8"?>
<!-- Planner Configuration -->

<Config>
    <m_acModules>
        <m_strctModule>
            <m_strctGeneral
                m_strName = "Electrode Planning"
                m_strInitFunc = "fnElectrodePlanningNewInit"
                m_strCallbackFunc = "fnElectrodePlanningNewCallback"
            > </m_strctGeneral>
            
            <m_acStereoModels>
                  <m_strctModel>
                    <m_strctGeneral
                        m_strModelName = "Kopf 1430"
                    > </m_strctGeneral>
                    <m_strctArm
                        m_strName = "Left Arm"
                        m_strModelFunction =  "fnKopf1460LeftArm"
                    > </m_strctArm>
                    <m_strctArm
                        m_strName = "Right Arm"
                        m_strModelFunction =  "fnKopf1460RightArm"
                    > </m_strctArm>

                 <m_strctArm
                        m_strName = "Right Arm (Fine calibrated)"
                        m_strModelFunction =  "fnKopf1460RightArm_MO"
                 > </m_strctArm>

                </m_strctModel>
                
                <m_strctModel>
                    <m_strctGeneral
                        m_strModelName = "Kopf 900A"
                    > </m_strctGeneral>
                    <m_strctArm
                        m_strName = "Left Arm"
                        m_strModelFunction =  "fnKopf900ALeftArmMod"
                    > </m_strctArm>
                </m_strctModel>
            </m_acStereoModels>
            
            
            <m_acChamberModels>

				<m_strctModel
                        m_strModelName = "DAG HP iD25.456 oD 30"
                        m_strModelFunc = "fnBuildDAGChamberModel(0, 25.455844122715714, 30, 25.455844122715714)"
                    > </m_strctModel>
				
                   <m_strctModel
                        m_strModelName = "DAG iD22 oD 27"
                        m_strModelFunc = "fnBuildDAGChamberModel(0, 22, 27, 22)"
                    > </m_strctModel>
								
                   <m_strctModel
                        m_strModelName = "Crist 0 Deg"
                        m_strModelFunc = "fnBuildCristChamberModel(0)"
                    > </m_strctModel>					

                    <m_strctModel
                        m_strModelName = "Rectangular 20 mm"
                        m_strModelFunc = "fnBuildRectangularChamberModel(20)"
                    > </m_strctModel>

                    <m_strctModel
                        m_strModelName = "Rectangular 10 mm"
                        m_strModelFunc = "fnBuildRectangularChamberModel(10)"
                    > </m_strctModel>

                    <m_strctModel
                        m_strModelName = "Utah 10x10 96 ch 400µm"
                        m_strModelFunc = "fnBuildRectangularChamberModel3D(5, 5, 1.5)"
                    > </m_strctModel>						
					
                    <m_strctModel
                        m_strModelName = "FMA36 Standard 400µm"
                        m_strModelFunc = "fnBuildRectangularChamberModel3D(1.8, 4, 10)"
                    > </m_strctModel>
 
                    <m_strctModel
                        m_strModelName = "FMA36 HD 250µm"
                        m_strModelFunc = "fnBuildRectangularChamberModel3D(1.6, 2.95, 10)"
                    > </m_strctModel>					
					
                   <m_strctModel
                        m_strModelName = "Crist 30 Deg"
                        m_strModelFunc = "fnBuildCristChamberModel(30)"
                    > </m_strctModel>
               
                   <m_strctModel
                        m_strModelName = "Plastic1 Cannula"
                        m_strModelFunc = "fnBuildCannulaChamberModel"
                    > </m_strctModel>
                    
                   <m_strctModel
                        m_strModelName = "Kopf 1460 Tip"
                        m_strModelFunc = "fnBuildManipulatorTipModel"
                    > </m_strctModel>                    
            </m_acChamberModels>
            
            
           <m_acGridModels>
               
               <m_strctModel>
                   <m_strctParams
                       m_strModelName = "Standard Circular"
                       m_strDefine = "fnDefineGridModel_Standard"
                       m_strBuildModel = "fnBuildGridModel_Standard"
                       m_strBuildMesh = "fnBuildGridMesh_Standard"
                       m_strDraw2D = "fnDraw2DGridModel_Standard"
                       m_strGUI = "GridHelper"
                   > </m_strctParams>

                   <m_strctSpecialInstance 
                       m_strName = "DAG"
                       m_strHoleDist = "0.8"
                       m_strHoleDiam = "0.43"
					   m_strGridHeight = "15"
                       m_strGridInnerDiam = "21.0"
					   m_strGridOuterDiam = "21.95"
                   > </m_strctSpecialInstance>				   
				   
                   <m_strctSpecialInstance 
                       m_strName = "Crist"
                       m_strHoleDist = "1.0"
                       m_strHoleDiam = "0.75"
                       m_strGridInnerDiam = "16.6"
                   > </m_strctSpecialInstance>
                   
                   <m_strctSpecialInstance 
                       m_strName = "CIT"
                       m_strHoleDist = "1.0"
                       m_strHoleDiam = "0.75"
                       m_strGridInnerDiam = "18"
                   > </m_strctSpecialInstance>
                   
                   
                   <m_strctSpecialInstance 
                       m_strName = "Shay 10 Deg v1.0"

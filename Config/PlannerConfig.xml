                       m_strHoleDist = "1.0154"
                       m_strHoleDiam = "0.75"
                       m_strGridInnerDiam = "18"
                   > </m_strctSpecialInstance>
                   
               </m_strctModel>
           
               
               <m_strctModel>
                   <m_strctParams
                       m_strModelName = "Dual Circular"
